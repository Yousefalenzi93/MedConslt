{
  "name": "medical-consultation-platform",
  "version": "1.0.0",
  "description": "Comprehensive medical consultation platform for doctors",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:netlify": "npm install --legacy-peer-deps && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "export": "next export",
    "deploy": "npm run build",
    "setup": "npm install",
    "demo": "npm run setup && npm run dev",
    "clean": "rimraf .next out",
    "reinstall": "npm run clean && npm install",
    "netlify-build": "npm run build",
    "preview": "npm run build && npx serve out",
    "analyze": "ANALYZE=true npm run build",
    "test-build": "node test-build.js",
    "test-build-win": "powershell -ExecutionPolicy Bypass -File test-build.ps1",
    "final-check": "node final-check.js",
    "quick-setup": "bash quick-setup.sh",
    "quick-setup-win": "powershell -ExecutionPolicy Bypass -File quick-setup.ps1",
    "setup-github": "bash setup-github.sh",
    "setup-github-win": "powershell -ExecutionPolicy Bypass -File setup-github.ps1",
    "test-database": "node test-database.js",
    "fix-netlify": "node fix-netlify-build.js",
    "test-build-quick": "node test-build-quick.js",
    "test-timeout-fix": "node test-timeout-fix.js",
    "fix-event-handlers": "node fix-event-handlers.js",
    "check-event-handlers": "node check-event-handlers.js",
    "final-build-test": "node final-build-test.js",
    "ultimate-fix": "node ultimate-fix.js",
    "deploy": "node deploy-to-github.js",
    "emergency-fix": "node emergency-fix.js",
    "timeout-fix": "node timeout-fix.js"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/node": "^20.10.5",

    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",

    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.48.2",
    "date-fns": "^2.30.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
